#include<stdio.h>
int main()
{
int i,n,a[100],max,min;
printf("enter limit=");
scanf("%d",&n);
printf("enter n numbers");
for(i=0;i<n;i++)
{
scanf("%d",&a[i]);
}
min=a[0];max=a[0];
for(i=1;i<n;i++)
{
	if(a[i]<min)
	min=a[i];
	if(a[i]>max)
	max=a[i];
}
printf("min nos=%d\t",min);
printf("max nos=%d\t",max);


}



