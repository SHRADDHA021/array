#include<stdio.h>
int main()
{
int i,n,a[100],cnt1=0,cnt2=0,num;
printf("enter limit=");
scanf("%d",&n);
for(i=0;i<n;i++)
{
printf("enter n numbers");
scanf("%d",&a[i]);
}
for(i=0;i<n;i++)
{
if(a[i]%2==0)
cnt1++;
else 
cnt2++;
}
printf("\noccurance of even=%d",cnt1);
printf("\noccurance of odd=%d",cnt2);
}



