#include<stdio.h>
int main()
{
int i,n,a[100],cnt=0,num;
printf("enter limit=");
scanf("%d",&n);
for(i=0;i<n;i++)
{
printf("enter n numbers");
scanf("%d",&a[i]);
}
printf("enter num to find occurance");
scanf("%d",&num);
for(i=0;i<n;i++)
{
if(a[i]==num)
cnt++;
}
printf("occurance of num=%d",cnt);
}



